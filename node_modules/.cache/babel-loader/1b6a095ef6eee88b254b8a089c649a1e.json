,WAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KA9CA;AAgDA,IAAA,IAhDA,kBAgDA;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,KAAA,SAAA,CAAA,KAAA,WAAA,CAAA,EAAA,KAAA,UAAA;AACA,QAAA,KAAA,CACA,GADA,CAEA,oEACA,KAAA,UAAA,CAAA,EAHA,EAIA,KAAA,UAJA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;AACA,SARA;AASA,OAXA,MAWA;AACA,aAAA,SAAA,CAAA,IAAA,CAAA,KAAA,UAAA;AACA,QAAA,KAAA,CACA,IADA,CAEA,iEAFA,EAGA,KAAA,UAHA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;AACA,SAPA;AAQA;;AACA,WAAA,KAAA;AACA;AAxEA;AA7DA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <v-data-table\r\n      :headers=\"headers\"\r\n      :items=\"ListDiary\"\r\n      sort-by=\"calories\"\r\n      class=\"elevation-1\"\r\n      :page.sync=\"page\"\r\n      :items-per-page=\"itemsPerPage\"\r\n      hide-default-footer\r\n      @page-count=\"pageCount = $event\"\r\n    >\r\n      <template #item.nhatky=\"{ item }\">\r\n        <router-link :to=\"'/chitietnhatky/id=' + item.idchitiet\">\r\n          {{ item.nhatky }}</router-link\r\n        >\r\n      </template>\r\n      <template v-slot:top>\r\n        <v-toolbar flat>\r\n          <div class=\"title\" style=\"padding-left: 45%;\">\r\n            <v-toolbar-title>Danh Sách Tuần</v-toolbar-title>\r\n          </div>\r\n          <v-spacer></v-spacer>\r\n          <v-dialog v-model=\"dialog\" max-width=\"500px\">\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n              <v-btn\r\n                dark\r\n                class=\"mb-2\"\r\n                v-bind=\"attrs\"\r\n                v-on=\"on\"\r\n                style=\"margin-top: 10%;\"\r\n              >\r\n                Tuần Mới\r\n              </v-btn>\r\n              <v-btn\r\n                style=\"margin-top: 9%; margin-right: 5%; background-color: darkcyan;\"\r\n              >\r\n                Export\r\n              </v-btn>\r\n            </template>\r\n            <v-card>\r\n              <v-card-title>\r\n                <span class=\"headline\">{{ formTitle }}</span>\r\n              </v-card-title>\r\n\r\n              <v-card-text>\r\n                <v-container>\r\n                  <v-row>\r\n                    <v-col cols=\"12\" sm=\"6\" md=\"12\">\r\n                      <v-text-field\r\n                        v-model=\"editedItem.tieude\"\r\n                        label=\"Tiêu đề tuần\"\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" sm=\"6\" md=\"12\">\r\n                      <v-text-field\r\n                        v-model=\"editedItem.ngaybatdau\"\r\n                        label=\"Ngày bắt đầu\"\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" sm=\"6\" md=\"12\">\r\n                      <v-text-field\r\n                        v-model=\"editedItem.ngayketthuc\"\r\n                        label=\"Ngày kết thúc\"\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" sm=\"6\" md=\"12\">\r\n                      <v-text-field\r\n                        v-model=\"editedItem.trangthai\"\r\n                        label=\"Trạng Thái\"\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-container>\r\n              </v-card-text>\r\n\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"close\">\r\n                  Cancel\r\n                </v-btn>\r\n                <v-btn color=\"blue darken-1\" text @click=\"save\">\r\n                  Save\r\n                </v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n          <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\r\n            <v-card>\r\n              <v-card-title class=\"headline\"\r\n                >Are you sure you want to delete this item?</v-card-title\r\n              >\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"closeDelete\"\r\n                  >Cancel</v-btn\r\n                >\r\n                <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\"\r\n                  >OK</v-btn\r\n                >\r\n                <v-spacer></v-spacer>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n        </v-toolbar>\r\n      </template>\r\n      <template v-slot:item.actions=\"{ item }\">\r\n        <v-icon small class=\"mr-2\" @click=\"editItem(item)\">\r\n          mdi-pencil\r\n        </v-icon>\r\n        <v-icon small @click=\"deleteItem(item)\">\r\n          mdi-delete\r\n        </v-icon>\r\n      </template>\r\n      <template v-slot:no-data>\r\n        <v-btn color=\"primary\" @click=\"initialize\">\r\n          Reset\r\n        </v-btn>\r\n      </template>\r\n    </v-data-table>\r\n\r\n    <div class=\"text-center pt-2\">\r\n      <v-pagination v-model=\"page\" :length=\"pageCount\"></v-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data: () => ({\r\n    dialog: false,\r\n    dialogDelete: false,\r\n    delete: 0,\r\n    page: 1,\r\n    pageCount: 0,\r\n    itemsPerPage: 4,\r\n    headers: [\r\n      {\r\n        text: \"STT\",\r\n        align: \"start\",\r\n\r\n        value: \"id\"\r\n      },\r\n      { text: \"Tiêu đề\", value: \"tieude\" },\r\n      { text: \"Ngày bắt đầu\", value: \"ngaybatdau\" },\r\n      { text: \"Ngày kết thúc\", value: \"ngayketthuc\" },\r\n      { text: \"Trạng thái\", value: \"trangthai\", sortable: false },\r\n      { text: \"Nhật ký theo tuần\", value: \"nhatky\" },\r\n      { text: \"Actions\", value: \"actions\", sortable: false }\r\n    ],\r\n    ListDiary: [],\r\n    editedIndex: -1,\r\n    editedItem: {\r\n      id: \"\",\r\n      tieude: \"\",\r\n      ngaybatdau: \"\",\r\n      ngayketthuc: \"\",\r\n      trangthai: \"Not approved\",\r\n      nhatky: \"See detail\"\r\n    },\r\n    defaultItem: {\r\n      id: \"\",\r\n      tieude: \"\",\r\n      ngaybatdau: \"\",\r\n      ngayketthuc: \"\",\r\n      trangthai: \"Not approved\",\r\n      nhatky: \"See detail\"\r\n    }\r\n  }),\r\n\r\n  computed: {\r\n    formTitle() {\r\n      return this.editedIndex === -1 ? \"New Item\" : \"Edit Item\";\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    dialog(val) {\r\n      val || this.close();\r\n    },\r\n    dialogDelete(val) {\r\n      val || this.closeDelete();\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.initialize();\r\n  },\r\n\r\n  methods: {\r\n    initialize() {\r\n      axios\r\n        .get(\"https://5fc999fb3c1c220016440daf.mockapi.io/user/nhatky/dstuan/\")\r\n        .then(response => {\r\n          this.ListDiary = response.data;\r\n        });\r\n    },\r\n    editItem(item) {\r\n      this.editedIndex = this.ListDiary.indexOf(item);\r\n      this.editedItem = Object.assign({}, item);\r\n      this.editedID = this.editedItem.id;\r\n      this.dialog = true;\r\n    },\r\n\r\n    deleteItem(item) {\r\n      this.dialogDelete = true;\r\n      this.editedIndex = this.ListDiary.indexOf(item);\r\n      this.editedItem = Object.assign({}, item);\r\n    },\r\n\r\n    deleteItemConfirm() {\r\n      axios.delete(\r\n        \"https://5fc999fb3c1c220016440daf.mockapi.io/user/nhatky/dstuan/\" +\r\n          this.editedItem.id\r\n      );\r\n      {\r\n        this.ListDiary.splice(this.editedIndex, 1);\r\n        this.closeDelete();\r\n      }\r\n    },\r\n\r\n    close() {\r\n      this.dialog = false;\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem);\r\n        this.editedIndex = -1;\r\n      }, 300);\r\n    },\r\n\r\n    closeDelete() {\r\n      this.dialogDelete = false;\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem);\r\n        this.editedIndex = -1;\r\n      });\r\n    },\r\n\r\n    save() {\r\n      if (this.editedIndex > -1) {\r\n        Object.assign(this.ListDiary[this.editedIndex], this.editedItem);\r\n        axios\r\n          .put(\r\n            \"https://5fc999fb3c1c220016440daf.mockapi.io/user/nhatky/dstuan/\" +\r\n              this.editedItem.id,\r\n            this.editedItem\r\n          )\r\n          .catch(error => {\r\n            console.log(error.response);\r\n          });\r\n      } else {\r\n        this.ListDiary.push(this.editedItem);\r\n        axios\r\n          .post(\r\n            \"https://5fc999fb3c1c220016440daf.mockapi.io/user/nhatky/dstuan/\",\r\n            this.editedItem\r\n          )\r\n          .catch(error => {\r\n            console.log(error.response);\r\n          });\r\n      }\r\n      this.close();\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/NhatKyThucTap"}]}